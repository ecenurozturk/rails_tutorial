<h1>all posts</h1>

<% @posts.each do |post| %>
<b><%= "#{post.id}. post " %></b>
<%=post.id %>
<%= post.header %>
<%= post.body %>
<%= link_to "Show", post_path(post) %>
<%= link_to "Edit", edit_post_path(post.id) %>
<%= link_to "Delete", post_path(post), method: :delete ,
data: { confirm: 'silmek istedigini emin misin?' }%>
<br>
<% end %>
<br>
<%= link_to 'New post',new_post_path %>
